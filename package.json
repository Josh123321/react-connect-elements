{
  "name": "react-connect-elements",
  "version": "0.0.1",
  "description": "Connect elements with SVG",
  "license": "MIT",
  "author": "Emerson Laurentino <emersonlaurentino@hotmail.com>",
  "homepage": "https://github.com/emersonlaurentino/react-connect-elements",
  "main": "dist/react-connect-elements.js",
  "source": "src/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "microbundle -o dist",
    "build:dev": "microbundle watch",
    "demo:build": "parcel build --out-dir demo/dist demo/index.html --public-url=/react-popper",
    "demo:deploy": "yarn demo:build && gh-pages -d demo/dist",
    "demo:dev": "parcel --out-dir demo/dist demo/index.html",
    "lint": "esw -c .eslintrc src --ext .js --ignore-path .eslintignore --color",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "precommit": "pretty-quick --staged && test",
    "prepare": "npm run build"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>jest.setup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emersonlaurentino/react-connect-elements"
  },
  "bugs": {
    "url": "https://github.com/emersonlaurentino/react-connect-elements/issues"
  },
  "keywords": [
    "react",
    "react-connect-elements",
    "component",
    "svg",
    "connect"
  ],
  "peerDependencies": {
    "react": "0.14.x || ^15.0.0 || ^16.0.0",
    "react-dom": "0.14.x || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.4",
    "gh-pages": "^1.1.0",
    "jest": "^22.4.3",
    "microbundle": "^0.4.4",
    "parcel-bundler": "^1.7.1",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1"
  }
}
